# my_favorite_sites.py

import webbrowser
dic={
"1":'https://www.facebook.com/',
"2":'https://web.whatsapp.com/',
"3":'https://www.youtube.com/'
"4":'https://stackoverflow.com/',
"5": https://www.reddit.com/'

}

def firefox (url):
     webbrowser.open_new_tab(url)

# main.py


import my_favorite_sites
from my_favorite_sites import firefox
from my_favorite_sites import dic

print("Enter the number of the website you will use:")
print("1 - Facebook")
print("2 - WhatsApp")
print("3 - YouTube")
print("4 - stackoverflow")
print("5 - reddit")

# Take user input
x = input("Enter your choice: ")

y = dic.get(x)
firefox(y)
